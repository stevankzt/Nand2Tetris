/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a= a, b= b, out= nandAB);
    Nand(a= nandAB, b= a, out= temp1);
    Nand(a= nandAB, b= b, out= temp2);
    Nand(a= temp1, b= temp2, out= out);

}
/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in= sel, out= notSel);
    
    Nand(a= a, b= notSel, out= aANDnotSelNot);
    Nand(a= b, b= sel, out= bAndSelNot);
    Nand(a= aANDnotSelNot, b= bAndSelNot, out= out);

 
    
}